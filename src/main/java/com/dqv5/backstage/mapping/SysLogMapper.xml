<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dqv5.backstage.dao.SysLogDao">
    <resultMap id="BaseResultMap" type="com.dqv5.backstage.model.SysLog">
        <id column="log_id" property="logId"/>
        <result column="user_id" property="userId"/>
        <result column="detail" property="detail"/>
        <result column="operate_time" property="operateTime"/>
    </resultMap>

    <insert id="save" parameterType="com.dqv5.backstage.model.SysLog">
        insert into sys_log ( log_id, user_id,detail,operate_time )
        values ( #{logId}, #{userId},#{detail},#{operateTime} )
    </insert>
    <select id="getByPrimaryKey" resultMap="BaseResultMap">
        SELECT * FROM sys_log where log_id =#{logId}
    </select>

    <select id="getAll" resultMap="BaseResultMap">
        SELECT * FROM sys_log
    </select>

</mapper>